{
    "extends": [
        "config:base"
    ],
    "assignAutomerge": true,
    "enabledManagers": [
        "composer"
    ],
    "dependencyDashboard": true,
    "rangeStrategy": "update-lockfile",
    "rebaseWhen": "conflicted",
    "schedule": [
        "after 7am and before 10pm every weekday"
    ],
    "timezone": "Europe/Amsterdam",
    "updateNotScheduled": false
}
